<div class="container">
  <div class="jumbotron">
      <div class="pull-right">
      </div>
      <p><h1>Trainers</h1></p>
  </div>

  <table class='table'>
    <thead>
      <tr>
        <th><strong>Trainer Name</strong></th>
      </tr>
    </thead>
    <% @trainers.each do |trainer| %>
    <tr>
      <td><%= link_to trainer.name, trainer_path(trainer) %></td>
    </tr>
    <% end %>
  </table>
</div>
